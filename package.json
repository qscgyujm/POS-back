    "@types/express": "^4.17.3",
    "@types/node": "^13.9.0",
    "aws-sdk": "^2.814.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mariadb": "^2.2.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
